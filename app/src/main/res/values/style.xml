<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="imgView_back">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/iconp_back</item>
    </style>

    <style name="tv_18sp">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/size_18sp</item>
    </style>

    <style name="tv_20sp">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/size_20sp</item>
    </style>

    <style name="tv_16sp">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/size_16sp</item>
        <item name="android:padding">5dp</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:drawableRight">@drawable/iconp_star</item>
        <item name="android:layout_marginStart">5dp</item>
    </style>

    <style name="tv_16sp_2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/size_16sp</item>
        <item name="android:padding">5dp</item>
    </style>
    <style name="tv_16sp_3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/size_16sp</item>
    </style>

    <style name="tv_toolbar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/size_20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">5dp</item>
    </style>

    <style name="edt1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/size_18sp</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>

    <style name="btn1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/size_16sp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginEnd">10dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="btn2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/size_18sp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginEnd">10dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:gravity">left</item>
        <item name="android:padding">17dp</item>
    </style>

    <style name="ll_gray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">4dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@color/text_gray2</item>
    </style>

    <style name="ll_gray_2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:background">@color/text_gray</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
    </style>


    <style name="btn_search">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/size_16sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="img_down">
        <item name="android:layout_width">17dp</item>
        <item name="android:layout_height">17dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:src">@drawable/iconp_down_black</item>
    </style>

    <style name="img_up">
        <item name="android:layout_width">17dp</item>
        <item name="android:layout_height">17dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:src">@drawable/iconp_up_black</item>
    </style>


    <style name="ll_search">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

</resources>